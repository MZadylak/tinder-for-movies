{
    "extends": ["airbnb"],
    "rules": {
        "react/jsx-filename-extension": [1, {
            "extensions": [".js"]
        }],
        "indent": ["error", 4],
        "react/jsx-indent": ["error", 4],
        "react/jsx-indent-props": ["error", 4],
        "no-console": "off"
    },
    "globals": {
        "window": true,
        "document": true
    }
}